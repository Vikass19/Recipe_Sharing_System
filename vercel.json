{
    "version": 2,
    "builds": [
      {
        "src": "Recipe_Sharing_System/wsgi.py",
        "use": "@vercel/python",
        "config": { "maxLambdaSize": "15mb" }
      }
    ],
    "routes": [
      { "src": "/static/(.*)", "dest": "/static/$1" },
      { "src": "/(.*)", "dest": "project_name/wsgi.py" }
    ]
  }
  